    <!-- https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_carrier_of -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_carrier_of">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
