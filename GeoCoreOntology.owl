        <rdfs:label xml:lang="en">site location role</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_unconsolidated_earth_material -->

