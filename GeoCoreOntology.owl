    <!-- https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_earth_fluid -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_earth_fluid">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_earth_material"/>
        <owl:disjointWith rdf:resource="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_rock"/>
        <obo:IAO_0000115 xml:lang="en">Earth fluid is an &apos;earth material&apos; that is fluild.

They can be water, oil, gas or a mixture of those fluids.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">earth fluid</rdfs:label>
        <rdfs:label xml:lang="pt">fluido da terra</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_earth_material -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_earth_material">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000040"/>
        <obo:IAO_0000115 xml:lang="en">Earth material is a &apos;material entity&apos; that is a natural amount of matter, which is solid, fluid or unconsolidated. Earth materials are generated by some &apos;geological process&apos;.

They are rigid entities with a definite identity criteria, but no unifying relation.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">earth material</rdfs:label>
        <rdfs:label xml:lang="pt">material geologico</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_genetic_role -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_genetic_role">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000023"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000054"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_geological_process"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">genetic role</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_geological_age -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_geological_age">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000019"/>
        <obo:IAO_0000115 xml:lang="en">It is a Quality of Geological Object related to a Geological Time Interval during which some Geological Process operated. The age of a Geological Object is the age of the process that generated this object.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">geological age</rdfs:label>
        <rdfs:label xml:lang="pt">idade geológica</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_geological_boundary -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_geological_boundary">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000140"/>
        <obo:IAO_0000115 xml:lang="en">A Geological Boundary is a Continuant Fiat Boundary corresponding to a physical discontinuity of any nature, located on the external surface of a Geological Object.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">geological boundary</rdfs:label>
        <rdfs:label xml:lang="pt">limite geológico</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_geological_contact -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_geological_contact">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000145"/>
        <obo:IAO_0000115 xml:lang="en">It is a Relational Quality which relates two Geologic Objects having the same dimension (1D, 2D, or 3D), whose external boundaries are adjacent to each other.</obo:IAO_0000115>
        <rdfs:label xml:lang="pt">contato geológico</rdfs:label>
        <rdfs:label xml:lang="en">geological contact</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_geological_object -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_geological_object">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000030"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_has_age"/>
                        <owl:someValuesFrom rdf:resource="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_geological_age"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_constituted_by"/>
                        <owl:allValuesFrom rdf:resource="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_earth_material"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_generated_by"/>
                        <owl:allValuesFrom rdf:resource="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_geological_process"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_has_genetic_role"/>
                        <owl:allValuesFrom rdf:resource="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_genetic_role"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_genetic_role_of"/>
                <owl:someValuesFrom rdf:resource="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_geological_object"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <obo:IAO_0000115 xml:lang="en">A Geological Object is a rigid entity that provides its own identity and holds a unity criteria. A Geological Object is constituted by Earth Material and is generated by some Geological Process.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">geological object</rdfs:label>
        <rdfs:label xml:lang="pt">objeto geológico</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_geological_process -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_geological_process">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000055"/>
                <owl:someValuesFrom rdf:resource="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_genetic_role"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115 xml:lang="en">It is a physical, chemical or biological process or series of processes that generate, destroy or modify the shape, the internal arrangement or the composition of at least one Geological Object.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">geological process</rdfs:label>
        <rdfs:label xml:lang="pt">processo geológico</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_geological_structure -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_geological_structure">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000031"/>
        <obo:IAO_0000115 xml:lang="en">It is a Generically Dependent Continuant which describes the internal arrangement of a Geological Object, i.e., the configuration and the mutual relationships of its different parts.</obo:IAO_0000115>
        <rdfs:label xml:lang="pt">estrutura geológica</rdfs:label>
        <rdfs:label xml:lang="en">geological structure</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_geological_time_interval -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_geological_time_interval">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000038"/>
        <obo:IAO_0000115 xml:lang="en">It is a Temporal Region of one dimension within the Global Geological Time. A Geological Time Interval may be occupied by one or several Geological Processes.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">geological time interval</rdfs:label>
        <rdfs:label xml:lang="pt">intervalo de tempo geológico</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_mineral -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_mineral">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_earth_material"/>
        <obo:IAO_0000115 xml:lang="en">Mineral is an &apos;earth material&apos; that is an inorganic, solid, and homogenous chemical compound with crystalline structure.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">mineral</rdfs:label>
        <rdfs:label xml:lang="pt">mineral</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_rock -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_rock">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_earth_material"/>
        <obo:IAO_0000115 xml:lang="en">Rock is a solid consolidated &apos;earth material&apos; that is made of mineral matter, or material of mineral origin.</obo:IAO_0000115>
        <obo:IAO_0000116 xml:lang="en">Geologists define rock at a scale of observation where they consider it a homogeneous mass.</obo:IAO_0000116>
        <rdfs:label xml:lang="pt">rocha</rdfs:label>
        <rdfs:label xml:lang="en">rock</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lucvaladares/GeoCoreOntology/master/GeoCoreOntology.owl#UFRGS_GCO_site_location_role -->

